#################################################################
#
#    Classes
#
#################################################################


###  http://conference#Abstract

:Abstract rdf:type owl:Class ;
          
          rdfs:label "Abstract" ;
          
          rdfs:subClassOf :Extended_abstract .



###  http://conference#Accepted_contribution

:Accepted_contribution rdf:type owl:Class ;
                       
                       rdfs:subClassOf :Reviewed_contribution ;
                       
                       owl:disjointWith :Rejected_contribution .



###  http://conference#Active_conference_participant

:Active_conference_participant rdf:type owl:Class ;
                               
                               rdfs:subClassOf :Conference_contributor ,
                                               :Conference_participant ,
                                               [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Invited_speaker
                                                               :Regular_author
                                                             )
                                               ] ;
                               
                               owl:disjointWith :Passive_conference_participant .



###  http://conference#Call_for_paper

:Call_for_paper rdf:type owl:Class ;
                
                rdfs:label "Call for papers" ;
                
                rdfs:subClassOf :Conference_document .



###  http://conference#Call_for_participation

:Call_for_participation rdf:type owl:Class ;
                        
                        rdfs:label "Call for participation" ;
                        
                        rdfs:subClassOf :Conference_document .



###  http://conference#Camera_ready_contribution

:Camera_ready_contribution rdf:type owl:Class ;
                           
                           rdfs:subClassOf :Accepted_contribution .



###  http://conference#Chair

:Chair rdf:type owl:Class ;
       
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :was_a_committee_chair_of ;
                             owl:someValuesFrom :Committee
                           ] ;
       
       rdfs:subClassOf :Committee_member .



###  http://conference#Co-chair

:Co-chair rdf:type owl:Class ;
          
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :was_a_committe_co-chair_of ;
                                owl:someValuesFrom :Committee
                              ] ;
          
          rdfs:subClassOf :Committee_member .



###  http://conference#Committee

:Committee rdf:type owl:Class ;
           
           rdfs:label "Conference Committee" ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( :Organizing_committee
                                               :Program_committee
                                               :Steering_committee
                                             )
                               ] ;
           
           rdfs:comment "Any conference committee, such as, e.g., the program committee." .



###  http://conference#Committee_member

:Committee_member rdf:type owl:Class ;
                  
