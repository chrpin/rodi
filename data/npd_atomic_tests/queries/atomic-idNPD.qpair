sql=SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x, CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING) AS y FROM facility_moveable TABLE1 WHERE TABLE1.fclNpdidFacility IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/discovery/', CAST(TABLE1.dscNpdidDiscovery AS CHARACTER VARYING), '/', CAST(TABLE1.dscReservesRC AS CHARACTER VARYING)) AS x, CAST(TABLE1.dscNpdidDiscovery AS CHARACTER VARYING) AS y FROM discovery_reserves TABLE1 WHERE TABLE1.dscNpdidDiscovery IS NOT NULL AND TABLE1.dscReservesRC IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x, CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING) AS y FROM facility_fixed TABLE1 WHERE TABLE1.fclNpdidFacility IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/petreg/facility/', CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING)) AS x, CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING) AS y FROM tuf_petreg_licence TABLE1 WHERE TABLE1.tufNpdidTuf IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/wellbore/', CAST(TABLE1.wlbNpdidWellbore AS CHARACTER VARYING)) AS x, CAST(TABLE1.wlbNpdidWellbore AS CHARACTER VARYING) AS y FROM wellbore_npdid_overview TABLE1 WHERE TABLE1.wlbNpdidWellbore IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/baa/', CAST(TABLE1.baaNpdidBsnsArrArea AS CHARACTER VARYING), '/polygon/', CAST(TABLE1.baaNpdidBsnsArrAreaPoly AS CHARACTER VARYING)) AS x, CAST(TABLE1.baaNpdidBsnsArrAreaPoly AS CHARACTER VARYING) AS y FROM baaArea TABLE1 WHERE TABLE1.baaNpdidBsnsArrArea IS NOT NULL AND TABLE1.baaNpdidBsnsArrAreaPoly IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/baa/', CAST(TABLE1.baaNpdidBsnsArrArea AS CHARACTER VARYING)) AS x, CAST(TABLE1.baaNpdidBsnsArrArea AS CHARACTER VARYING) AS y FROM bsns_arr_area TABLE1 WHERE TABLE1.baaNpdidBsnsArrArea IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.fldNpdidField AS CHARACTER VARYING)) AS x, CAST(TABLE1.fldNpdidField AS CHARACTER VARYING) AS y FROM field TABLE1 WHERE TABLE1.fldNpdidField IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/licence/', CAST(TABLE1.prlNpdidLicence AS CHARACTER VARYING)) AS x, CAST(TABLE1.prlNpdidLicence AS CHARACTER VARYING) AS y FROM licence TABLE1 WHERE TABLE1.prlNpdidLicence IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/discovery/', CAST(TABLE1.dscNpdidDiscovery AS CHARACTER VARYING)) AS x, CAST(TABLE1.dscNpdidDiscovery AS CHARACTER VARYING) AS y FROM discovery TABLE1 WHERE TABLE1.dscNpdidDiscovery IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING)) AS x, CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING) AS y FROM tuf_owner_hst TABLE1 WHERE TABLE1.tufNpdidTuf IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.fldNpdidField AS CHARACTER VARYING), '/reserve') AS x, CAST(TABLE1.fldNpdidField AS CHARACTER VARYING) AS y FROM field_reserves TABLE1 WHERE TABLE1.fldNpdidField IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/survey/', CAST(TABLE1.seaName AS CHARACTER VARYING)) AS x, CAST(TABLE1.seaNpdidSurvey AS CHARACTER VARYING) AS y FROM seis_acquisition TABLE1 WHERE TABLE1.seaName IS NOT NULL AND TABLE1.seaNpdidSurvey IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/stratum/', CAST(TABLE1.lsuNpdidLithoStrat AS CHARACTER VARYING)) AS x, CAST(TABLE1.lsuNpdidLithoStrat AS CHARACTER VARYING) AS y FROM wellbore_formation_top TABLE1 WHERE TABLE1.lsuNpdidLithoStrat IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/pipeline/', CAST(TABLE1.pipNpdidPipe AS CHARACTER VARYING)) AS x, CAST(TABLE1.pipNpdidPipe AS CHARACTER VARYING) AS y FROM pipLine TABLE1 WHERE TABLE1.pipNpdidPipe IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/company/', CAST(TABLE1.cmpNpdidCompany AS CHARACTER VARYING)) AS x, CAST(TABLE1.cmpNpdidCompany AS CHARACTER VARYING) AS y FROM company TABLE1 WHERE TABLE1.cmpNpdidCompany IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING)) AS x, CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING) AS y FROM tuf_operator_hst TABLE1 WHERE TABLE1.tufNpdidTuf IS NOT NULL
