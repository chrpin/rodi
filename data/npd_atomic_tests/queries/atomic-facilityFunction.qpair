sql=SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x, CAST(TABLE1.fclFunctions AS CHARACTER VARYING) AS y FROM facility_moveable TABLE1 WHERE TABLE1.fclFunctions IS NOT NULL AND TABLE1.fclNpdidFacility IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x, CAST(TABLE1.fclFunctions AS CHARACTER VARYING) AS y FROM facility_fixed TABLE1 WHERE TABLE1.fclFunctions IS NOT NULL AND TABLE1.fclNpdidFacility IS NOT NULL
