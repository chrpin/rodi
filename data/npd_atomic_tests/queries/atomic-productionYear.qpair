sql=SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.prfNpdidInformationCarrier AS CHARACTER VARYING), '/production/', CAST(TABLE1.prfYear AS CHARACTER VARYING)) AS x, CAST(TABLE1.prfYear AS CHARACTER VARYING) AS y FROM field_production_yearly TABLE1 WHERE TABLE1.prfNpdidInformationCarrier IS NOT NULL AND TABLE1.prfYear IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/ncs/production/', CAST(TABLE1.prfYear AS CHARACTER VARYING), '/', CAST(TABLE1.prfMonth AS CHARACTER VARYING)) AS x, CAST(TABLE1.prfYear AS CHARACTER VARYING) AS y FROM field_production_totalt_NCS_month TABLE1 WHERE TABLE1.prfYear IS NOT NULL AND TABLE1.prfMonth IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.prfNpdidInformationCarrier AS CHARACTER VARYING), '/investment/', CAST(TABLE1.prfYear AS CHARACTER VARYING)) AS x, CAST(TABLE1.prfYear AS CHARACTER VARYING) AS y FROM field_investment_yearly TABLE1 WHERE TABLE1.prfNpdidInformationCarrier IS NOT NULL AND TABLE1.prfYear IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.prfNpdidInformationCarrier AS CHARACTER VARYING), '/production/', CAST(TABLE1.prfYear AS CHARACTER VARYING), '/', CAST(TABLE1.prfMonth AS CHARACTER VARYING)) AS x, CAST(TABLE1.prfYear AS CHARACTER VARYING) AS y FROM field_production_monthly TABLE1 WHERE TABLE1.prfNpdidInformationCarrier IS NOT NULL AND TABLE1.prfYear IS NOT NULL AND TABLE1.prfMonth IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/ncs/production/', CAST(TABLE1.prfYear AS CHARACTER VARYING)) AS x, CAST(TABLE1.prfYear AS CHARACTER VARYING) AS y FROM field_production_totalt_NCS_year TABLE1 WHERE TABLE1.prfYear IS NOT NULL
