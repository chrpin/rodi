sql=SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.fldNpdidField AS CHARACTER VARYING), '/activity/', CAST(TABLE1.fldStatus AS CHARACTER VARYING), '/history/', CAST(TABLE1.fldStatusFromDate AS CHARACTER VARYING), '/', CAST(TABLE1.fldStatusToDate AS CHARACTER VARYING)) AS x, CAST(TABLE1.fldStatus AS CHARACTER VARYING) AS y FROM field_activity_status_hst TABLE1 WHERE TABLE1.fldStatus IS NOT NULL AND TABLE1.fldNpdidField IS NOT NULL AND TABLE1.fldStatusFromDate IS NOT NULL AND TABLE1.fldStatusToDate IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.fldNpdidField AS CHARACTER VARYING)) AS x, CAST(TABLE1.fldCurrentActivitySatus AS CHARACTER VARYING) AS y FROM field TABLE1 WHERE TABLE1.fldNpdidField IS NOT NULL AND TABLE1.fldCurrentActivitySatus IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/licence/', CAST(TABLE1.prlNpdidLicence AS CHARACTER VARYING), '/task/', CAST(TABLE1.prlTaskID AS CHARACTER VARYING)) AS x, CAST(TABLE1.prlTaskStatusEn AS CHARACTER VARYING) AS y FROM licence_task TABLE1 WHERE TABLE1.prlTaskStatusEn IS NOT NULL AND TABLE1.prlTaskID IS NOT NULL AND TABLE1.prlNpdidLicence IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/licence/', CAST(TABLE1.prlNpdidLicence AS CHARACTER VARYING), '/task/', CAST(TABLE1.prlTaskID AS CHARACTER VARYING)) AS x, CAST(TABLE1.prlTaskStatusNo AS CHARACTER VARYING) AS y FROM licence_task TABLE1 WHERE TABLE1.prlTaskStatusNo IS NOT NULL AND TABLE1.prlTaskID IS NOT NULL AND TABLE1.prlNpdidLicence IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/wellbore/', CAST(TABLE1.wlbNpdidWellbore AS CHARACTER VARYING)) AS x, CAST(TABLE1.wlbStatus AS CHARACTER VARYING) AS y FROM wellbore_exploration_all TABLE1 WHERE TABLE1.wlbStatus IS NOT NULL AND TABLE1.wlbNpdidWellbore IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/licence/', CAST(TABLE1.prlNpdidLicence AS CHARACTER VARYING), '/phase/', CAST(TABLE1.prlPhase AS CHARACTER VARYING), '/history/', CAST(TABLE1.prlDatePhaseValidFrom AS CHARACTER VARYING), '/', CAST(TABLE1.prlDatePhaseValidTo AS CHARACTER VARYING)) AS x, CAST(TABLE1.prlPhase AS CHARACTER VARYING) AS y FROM licence_phase_hst TABLE1 WHERE TABLE1.prlPhase IS NOT NULL AND TABLE1.prlNpdidLicence IS NOT NULL AND TABLE1.prlDatePhaseValidFrom IS NOT NULL AND TABLE1.prlDatePhaseValidTo IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/discovery/', CAST(TABLE1.dscNpdidDiscovery AS CHARACTER VARYING)) AS x, CAST(TABLE1.dscCurrentActivityStatus AS CHARACTER VARYING) AS y FROM discovery TABLE1 WHERE TABLE1.dscNpdidDiscovery IS NOT NULL AND TABLE1.dscCurrentActivityStatus IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x, CAST(TABLE1.fclPhase AS CHARACTER VARYING) AS y FROM facility_fixed TABLE1 WHERE TABLE1.fclNpdidFacility IS NOT NULL AND TABLE1.fclPhase IS NOT NULL
