sql=SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/field/', CAST(TABLE1.fldNpdidField AS CHARACTER VARYING), '/reserve') AS x, CAST(TABLE1.fldRecoverableGas AS CHARACTER VARYING) AS y FROM field_reserves TABLE1 WHERE TABLE1.fldNpdidField IS NOT NULL AND TABLE1.fldRecoverableGas IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/discovery/', CAST(TABLE1.dscNpdidDiscovery AS CHARACTER VARYING), '/', CAST(TABLE1.dscReservesRC AS CHARACTER VARYING)) AS x, CAST(TABLE1.dscRecoverableGas AS CHARACTER VARYING) AS y FROM discovery_reserves TABLE1 WHERE TABLE1.dscRecoverableGas IS NOT NULL AND TABLE1.dscNpdidDiscovery IS NOT NULL AND TABLE1.dscReservesRC IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/company/', CAST(TABLE1.cmpNpdidCompany AS CHARACTER VARYING), '/reserve/', CAST(TABLE1.fldNpdidField AS CHARACTER VARYING)) AS x, CAST(TABLE1.cmpRecoverableGas AS CHARACTER VARYING) AS y FROM company_reserves TABLE1 WHERE TABLE1.cmpRecoverableGas IS NOT NULL AND TABLE1.cmpNpdidCompany IS NOT NULL AND TABLE1.fldNpdidField IS NOT NULL
sparql=SELECT ?x ?y { ?x <http://sws.ifi.uio.no/vocab/npd-v2#recoverableGas> ?y } 
