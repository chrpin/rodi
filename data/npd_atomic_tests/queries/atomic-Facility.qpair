sql=SELECT COUNT(*) FROM (SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING)) AS x FROM tuf_operator_hst TABLE1 WHERE TABLE1.tufNpdidTuf IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING)) AS x FROM tuf_owner_hst TABLE1 WHERE TABLE1.tufNpdidTuf IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/pipeline/', CAST(TABLE1.pipNpdidPipe AS CHARACTER VARYING)) AS x FROM pipLine TABLE1 WHERE TABLE1.pipNpdidPipe IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/petreg/facility/', CAST(TABLE1.tufNpdidTuf AS CHARACTER VARYING)) AS x FROM tuf_petreg_licence TABLE1 WHERE TABLE1.tufNpdidTuf IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x FROM facility_fixed TABLE1 WHERE TABLE1.fclNpdidFacility IS NOT NULL UNION ALL SELECT CONCAT('http://sws.ifi.uio.no/data/npd-v2/facility/', CAST(TABLE1.fclNpdidFacility AS CHARACTER VARYING)) AS x FROM facility_moveable TABLE1 WHERE TABLE1.fclNpdidFacility IS NOT NULL) AS T
